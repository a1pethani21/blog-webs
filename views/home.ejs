<%- include('partials/header') %>

    <title>Home</title>
    <div class="container-fluid">
        <h1>Home</h1>
        <p>
            <%=startC%>
        </p>



        <% posts.forEach(function(post){%>
            <h1>
                <%=post.title%>
            </h1>
            <p>
                <%=post.Post.substring(0,100) + "..." %>
            </p>
            <a href="/posts/<%=post.title%>">read more</a>
            <%})%>



    </div>
    <%- include("partials/footer") %>